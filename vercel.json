{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node@2.5.10"
    }
  ],
  "routes": [
    {
      "src": "^/public/(.*)",
      "dest": "/public/$1",
      "continue": true
    },
    {
      "src": "^/(.*)\\.(js|jsx|css|html|png|jpg|jpeg|gif|ico|svg|json)$",
      "dest": "/public/$1.$2"
    },
    {
      "src": ".*",
      "dest": "/index.js"
    }
  ]
}
